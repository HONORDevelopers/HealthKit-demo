<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hihonor.healthkitdemo">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.heartstudydemo"
        tools:targetApi="30">
        <activity
            android:name="com.hihonor.healthkitdemo.activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.RealTimeHeartRateActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.SubscribeActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.HealthSignsActivity"
            android:exported="false"
            android:theme="@style/Theme.heartstudydemo"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.HeartHealthActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.StressStatisticActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.BloodActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.BloodPressureActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.BloodOxygenActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.BodyTemperatureActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.MenstrualCycleActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.DailyActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.DailyDetailsActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.TranslucentDecor"/>
        <activity
            android:name="com.hihonor.healthkitdemo.activity.DailyActivitiesDataActivity"
            android:exported="false"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>

        <activity
            android:name="com.hihonor.healthkitdemo.activity.SleepActivity"
            android:configChanges="orientation|keyboardHidden|screenSize|smallestScreenSize|screenLayout"
            android:exported="false"
            android:screenOrientation="behind"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>

        <activity
            android:name="com.hihonor.healthkitdemo.activity.SleepDetailActivity"
            android:configChanges="orientation|keyboardHidden|screenSize|smallestScreenSize|screenLayout"
            android:exported="false"
            android:screenOrientation="behind"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>

        <activity
            android:name="com.hihonor.healthkitdemo.activity.HistorySportDataActivity"
            android:configChanges="orientation|keyboardHidden|screenSize|smallestScreenSize|screenLayout"
            android:exported="false"
            android:screenOrientation="behind"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>

        <activity
            android:name="com.hihonor.healthkitdemo.activity.SportRecordDetailActivity"
            android:configChanges="orientation|keyboardHidden|screenSize|smallestScreenSize|screenLayout"
            android:exported="false"
            android:screenOrientation="behind"
            android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen"/>
    </application>

</manifest>