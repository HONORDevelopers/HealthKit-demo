<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/item_detail_start_time"
        android:layout_width="0dp"
        android:layout_weight="5"
        android:layout_height="wrap_content"
        android:text="2022-11-19 22:00"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/location_detail_latitude"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="4"
        android:layout_marginStart="6dp"
        android:text="100"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/location_detail_longitude"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="4"
        android:layout_marginStart="6dp"
        android:text="100"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/location_detail_speed"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_marginStart="6dp"
        android:text="100"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/location_detail_precision"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_marginStart="6dp"
        android:text="100"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/location_detail_gpstra"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_marginStart="6dp"
        android:text="100"
        android:textSize="12sp" />
</LinearLayout>